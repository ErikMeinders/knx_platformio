{
  "name": "knx_platformio",
  "version": "1.0.3",
  "description": "A comprehensive platform for building KNX applications for ESP8266/ESP32 devices with advanced networking, web interfaces, OTA updates, and monitoring capabilities",
  "keywords": [
    "knx",
    "esp8266",
    "esp32",
    "platformio",
    "home-automation",
    "iot",
    "websocket",
    "webserver"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ErikMeinders/knx_platformio.git"
  },
  "authors": [
    {
      "name": "Erik Meinders",
      "email": "erik@easytocloud.com",
      "maintainer": true
    }
  ],
  "license": "MIT",
  "frameworks": "arduino",
  "platforms": [
    "espressif8266",
    "espressif32"
  ],
  "export": {
    "include": [
      "src/knxp_*",
      "include/knxp_*",
      "library.json",
      "CHANGELOG.md",
      "README.md",
      "LICENSE"
    ],
    "exclude": [
      "tests",
      "cicd",
      ".pio*"
    ]
  },
  "dependencies": [
    {
      "owner": "thelsing",
      "name": "knx",
      "version": "*"
    },
    {
      "owner": "erikmeinders",
      "name": "profiler",
      "version": "^0.7.4"
    },
    {
      "owner": "thijse",
      "name": "ArduinoLog",
      "version": "^1.1.1"
    },
    {
      "owner": "jandrassy",
      "name": "TelnetStream",
      "version": "1.2.5"
    },
    {
      "name": "WiFiManager",
      "version": "https://github.com/tzapu/WiFiManager.git"
    },
    {
      "name": "ArduinoJson",
      "version": "^7.1.0"
    },
    {
      "owner": "Links2004",
      "name": "WebSockets",
      "version": "^2.6.1"
    },
    {
      "owner": "me-no-dev",
      "name": "ESPAsyncTCP",
      "version": "https://github.com/me-no-dev/ESPAsyncTCP.git",
      "platforms": ["espressif8266"]
    },
    {
      "owner": "me-no-dev",
      "name": "AsyncTCP",
      "version": "https://github.com/me-no-dev/AsyncTCP.git",
      "platforms": ["espressif32"]
    },
    {
      "owner": "me-no-dev",
      "name": "ESPAsyncWebServer",
      "version": "https://github.com/me-no-dev/ESPAsyncWebServer.git"
    },
    {
      "owner": "alanswx",
      "name": "ESPAsyncWiFiManager",
      "version": "https://github.com/alanswx/ESPAsyncWiFiManager.git"
    }
  ],
  "build": {
    "libArchive": false,
    "flags": [
      "-I$LIBRARY_DIR/include"
    ]
  },
  "examples": [
    "examples/*"
  ]
}
